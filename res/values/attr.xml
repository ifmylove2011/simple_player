<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="front_color" format="color|reference" />
    <attr name="back_color" format="color|reference" />
    <attr name="count" format="integer" />
    <attr name="line_width" format="dimension|reference" />
    <attr name="icon" format="reference" />

    <declare-styleable name="VolumeView">
        <attr name="front_color" />
        <attr name="back_color" />
        <attr name="count" />
        <attr name="line_width" />
        <attr name="icon" />
    </declare-styleable>
    
    <attr name="firstColor" format="color" />
    <attr name="secondColor" format="color" />
    <attr name="circleWidth" format="dimension" />
    <attr name="dotCount" format="integer" />
    <attr name="splitSize" format="integer" />
    <attr name="bg" format="reference"></attr>

    <declare-styleable name="CustomVolumControlBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="dotCount" />
        <attr name="splitSize" />
        <attr name="bg" />
    </declare-styleable>

</resources>